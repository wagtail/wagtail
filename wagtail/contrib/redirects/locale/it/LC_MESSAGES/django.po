msgid ""
"Found %(total)s redirects, created %(successes)s and found %(errors)s errors."
msgstr ""
"Trovate %(total)s redirezioni, create %(successes)s e trovati %(errors)s "
"errori."

msgid "From"
msgstr "Da"

msgid "To"
msgstr "A"

msgid "Error"
msgstr "Errore"

msgid "Continue"
msgstr "Continua"

msgid "Export redirects"
msgstr "Esporta reindirizzamenti"

msgid "Site"
msgstr "Sito"

msgid "Edit this redirect"
msgstr "Modifica questo reindirizzamento"

msgid "No redirects found."
msgstr "Nessun reindirizzamento trovato"

