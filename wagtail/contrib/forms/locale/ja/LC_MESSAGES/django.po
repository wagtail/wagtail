msgstr " 複数のチェックボックス"

msgid "Drop down"
msgstr "セレクトボックス"

msgid "Multiple select"
msgstr "複数セレクター"

msgid "Radio buttons"
msgstr "ラジオボタン"

msgid "Date"
msgstr "日付"

msgid "Date/time"
msgstr "日時"

msgid "Hidden field"
msgstr "非表示フィールド"

