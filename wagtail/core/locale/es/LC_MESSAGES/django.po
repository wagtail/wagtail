msgstr "{label}: este bloque no tiene opciones."

msgid "This block has no options."
msgstr "Este bloque no tiene opciones."

#, python-format
msgid "The minimum number of items is %d"
msgstr "El mínimo número de elementos es %d"

msgid "This field is required."
msgstr "Este campo es necesario."

