msgstr "ئىشلەتكۈچى تەرجىمىھالى"

#, python-format
msgid "Assign role to 1 user"
msgid_plural "Assign role to %(counter)s users"
msgstr[0] "1 ئىشلەتكۈچىگە رول تەقسىملەيدۇ"
msgstr[1] "%(counter)s ئىشلەتكۈچىگە رول تەقسىملەيدۇ"

msgid "Assign role"
msgstr "رول تەقسىملەيدۇ"

msgid "Are you sure you want to assign this role to these users?"
msgstr "بۇ رولنى راستتىنلا مەزكۇر ئىشلەتكۈچىگە تەقسىملەمسىز؟"

msgid "You don't have permission to edit this user"
msgid_plural "You don't have permission to edit these users"
msgstr[0] "بۇ ئىشلەتكۈچىنى تەھرىرلەش ئىجازىتىڭىز يوق"
msgstr[1] "بۇ ئىشلەتكۈچىنى تەھرىرلەش ئىجازىتىڭىز يوق"

msgid "Yes, assign"
msgstr "ھەئە، تەقسىملە"

msgid "No, don't assign"
msgstr "ياق، تەقسىملىمە"

#, python-format
msgid "Delete 1 user"
msgid_plural "Delete %(counter)s users"
msgstr[0] "1 ئىشلەتكۈچىنى ئۆچۈر"
msgstr[1] "%(counter)s ئىشلەتكۈچىنى ئۆچۈر"

msgid "Delete"
msgstr "ئۆچۈر"

msgid "Are you sure you want to delete these users?"
msgstr "بۇ ئىشلەتكۈچىنى راستتىنلا ئۆچۈرەمسىز؟"

msgid "You don't have permission to delete this user"
msgid_plural "You don't have permission to delete these users"
msgstr[0] "بۇ  ئىشلەتكۈچىنى ئۆچۈرۈش ئىجازىتىڭىز يوق"
msgstr[1] "بۇ  ئىشلەتكۈچىنى ئۆچۈرۈش ئىجازىتىڭىز يوق"

msgid "Yes, delete"
msgstr "ھەئە، ئۆچۈر"

msgid "No, don't delete"
msgstr "ياق، ئۆچۈرمە"

#, python-format
msgid "Change active state for 1 user"
msgid_plural "Change active state for %(counter)s users"
msgstr[0] "1 ئىشلەتكۈچىنىڭ ئاكتىپ ھالىتىنى ئۆزگەرتىدۇ"
msgstr[1] "%(counter)s ئىشلەتكۈچىنىڭ ئاكتىپ ھالىتىنى ئۆزگەرتىدۇ"

msgid "Set active state"
msgstr "ئاكتىپ ھالەت تەڭشەك"

msgid "Are you sure you want to change the active state for these users?"
msgstr "بۇ ئىشلەتكۈچىلەرنىڭ ئاكتىپ ھالىتىنى راستتىنلا ئۆزگەرتەمسىز؟"

msgid "You cannot change your own active status"
msgstr "ئۆزىڭىزنىڭ ئاكتىپ ھالىتىنى ئۆزگەرتەلمەيسىز."

msgid "Yes, change state"
msgstr "ھەئە، ھالەتنى ئۆزگەرت"

msgid "No, don't change state"
msgstr "ياق ھالەتنى ئۆزگەرتمە"

