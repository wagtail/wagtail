msgstr "Elimina reindirizzamento %(title)s"

msgid "Delete"
msgstr "Elimina"

msgid "Are you sure you want to delete this redirect?"
msgstr "Sei sicuro di voler eliminare questo reindirizzamento?"

msgid "Yes, delete"
msgstr "Si, elimina"

msgid "Confirm import"
msgstr "Conferma importazione"

msgid "Confirm"
msgstr "Conferma"

msgid "Cancel"
msgstr "Annulla"

msgid "Preview"
msgstr "Anteprima"

