msgid "File format of type \"%(extension)s\" is not supported"
msgstr "Формат файлу \"%(extension)s\" не підтримується"

#, python-format
msgid "Imported file has a wrong encoding: %(error_message)s"
msgstr "Імпортований файл має неправильне кодування: %(error_message)s"

#, python-format
msgid "%(error)s encountered while trying to read file: %(filename)s"
msgstr "%(error)s помилок при читанні файлу: %(filename)s "

#, python-format
msgid "Imported %(total)d redirect"
msgid_plural "Imported %(total)d redirects"
msgstr[0] "Імпортовано %(total)d перенапрвлення"
msgstr[1] "Імпортовано %(total)d перенапрвлень"
