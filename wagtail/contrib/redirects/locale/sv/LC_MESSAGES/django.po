#, python-format
msgid "Sorry, no redirects match \"<em>%(query_string)s</em>\""
msgstr "Ledsen, inga omdirigeringar matchar \"<em>%(query_string)s</em>\""

#, python-format
msgid ""
"No redirects have been created. Why not <a href="
"\"%(wagtailredirects_add_redirect_url)s\">add one</a>?"
msgstr ""
"Inga omdirigeringar har skapats. Varför inte <a href="
"\"%(wagtailredirects_add_redirect_url)s\">skapa en</a>?"

msgid "Search redirects"
msgstr "Sökomdirigeringar"

msgid "Redirect '{0}' updated."
msgstr "Omdirigering '{0}' uppdaterad."

msgid "Edit"
msgstr "Editera"

msgid "The redirect could not be saved due to errors."
msgstr "Omdirigeringen kunde inte sparas p.g.a. fel."

msgid "Redirect '{0}' deleted."
msgstr "Omdirigering '{0}' raderad."

msgid "Redirect '{0}' added."
msgstr "Omdirigering '{0}' skapad."

msgid "The redirect could not be created due to errors."
msgstr "Omdirigeringen kunde inte sparas p.g.a. fel."
