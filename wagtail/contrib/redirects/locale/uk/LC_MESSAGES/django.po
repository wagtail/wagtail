msgstr "Перенаправлення не може бути збережене через помилки."

#, python-format
msgid "Redirect '%(redirect_title)s' updated."
msgstr "Перенаправлення '%(redirect_title)s' оновлено."

#, python-format
msgid "Redirect '%(redirect_title)s' deleted."
msgstr "Перенаправлення '%(redirect_title)s' видалено."

msgid "The redirect could not be created due to errors."
msgstr "Перенаправлення не може бути створене через помилки."

#, python-format
msgid "Redirect '%(redirect_title)s' added."
msgstr "Перенаправлення '%(redirect_title)s' додано."

msgid "Search redirects"
msgstr "Пошук перенаправлень"

