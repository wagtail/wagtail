{
  "compilerOptions": {
    "jsx": "react",
    "lib": ["es2015", "dom"],
    "noImplicitAny": false, // TODO: Enable once all existing code is typed
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "allowJs": true,
    "downlevelIteration": true
  },
  "include": ["src", "wagtail"],
  "exclude": ["mode_modules", "static"]
}
