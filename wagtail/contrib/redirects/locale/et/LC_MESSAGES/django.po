msgstr "Kustuta ümbersuunamine %(title)s"

msgid "Delete"
msgstr "Kustuta"

msgid "Are you sure you want to delete this redirect?"
msgstr "Kas soovite selle ümbersuunamise kindlasti kustutada?"

msgid "Yes, delete"
msgstr "Jah, kustuta"

msgid "Confirm import"
msgstr "Kinnitage import"

msgid "Confirm"
msgstr "Kinnitage"

msgid "Cancel"
msgstr "Tühista"

msgid "Preview"
msgstr "Eelvaade"

