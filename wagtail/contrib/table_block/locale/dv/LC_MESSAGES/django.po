msgid "Wagtail table block"
msgstr "ވެގްޓޭލް ޓޭބަލް ބްލޮކް"

msgid "Row header"
msgstr "ސަފުގެ ހެޑަރ"

msgid "Table headers"
msgstr "ތާވަލުގެ ހެޑަރ"

msgid "Display the first row as a header"
msgstr "ފުރަތަމަ ސަފު ހެޑަރެއްގެ ގޮތުގައި ދައްކާ"

msgid "Display the first column as a header"
msgstr "ފުރަތަމަ ކޮލަމް ހެޑަރެއްގެ ގޮތުގައި ދައްކާ"

msgid "Column header"
msgstr "ކޮލަމް ހެޑަރ"

msgid "Display the first row AND first column as headers"
msgstr "ފުރަތަމަ ސަފު އަދި ފުރަތަމަ ކޮލަމް ހެޑަރެއްގެ ގޮތުގައި ދައްކާ"

msgid "No headers"
msgstr "އެއްވެސް ހެޑަރ އެއް ނެތް"

msgid "Which cells should be displayed as headers?"
msgstr "ހެޑަރެއްގެ ގޮތުގައި ދައްކަންވާނީ ކޮން ސެލްތަކެއް"

msgid "Table caption"
msgstr "ތާވަލް ކެޕްޝަން"

msgid ""
"A heading that identifies the overall topic of the table, and is useful for "
"screen reader users."
msgstr "ތާވަލުގެ ޖުމްލަ މައުޟޫޢު ދެނެގަނެ، އަދި ސްކްރީން ރީޑަރ ބޭނުންކުރާ ފަރާތްތަކަށް ބޭނުންތެރި ސުރުޚީއެއް"

msgid "Table"
msgstr "ތާވަލު"
