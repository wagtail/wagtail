msgstr[0] "Jest %(counter)s dopasowanie"
msgstr[1] "Jest %(counter)s dopasowań"
msgstr[2] "Jest %(counter)s dopasowań"
msgstr[3] "Jest %(counter)s dopasowań"

#, python-format
msgid "Sorry, no redirects match \"<em>%(query_string)s</em>\""
msgstr ""
"Przepraszamy, żadne przekierowanie nie pasuje do \"<em>%(query_string)s</em>"
"\""

#, python-format
msgid ""
"No redirects have been created. Why not <a href="
"\"%(wagtailredirects_add_redirect_url)s\">add one</a>?"
msgstr ""
"Nie zostały stworzone żadne przekierowania. Czemu nie <a href="
"\"%(wagtailredirects_add_redirect_url)s\">dodać jakiegoś</a>?"

msgid "Search redirects"
msgstr "Szukaj przekierowań"

#, python-format
msgid "Redirect '%(redirect_title)s' updated."
msgstr "Uaktualniono przekierowanie '%(redirect_title)s'."

msgid "Edit"
msgstr "Edytuj"

msgid "The redirect could not be saved due to errors."
msgstr "Przekierowanie nie mogło zostać zapisane z powodu błędów."

#, python-format
msgid "Redirect '%(redirect_title)s' deleted."
msgstr "Usunięto przekierowanie '%(redirect_title)s'."

#, python-format
msgid "Redirect '%(redirect_title)s' added."
msgstr "Przekierowanie '%(redirect_title)s' dodane."

msgid "The redirect could not be created due to errors."
msgstr "Przekierowanie nie mogło zostać stworzone z powodu błędów."

