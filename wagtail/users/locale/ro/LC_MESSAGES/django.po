#, python-format
msgid "Assign role to 1 user"
msgid_plural "Assign role to %(counter)s users"
msgstr[0] "Adaugă rol la 1 utilizator"
msgstr[1] "Adaugă rol la %(counter)s utilizatori"
msgstr[2] "Adaugă rol la %(counter)s utilizatori"

msgid "Assign role"
msgstr "Adaugă rol"

msgid "Are you sure you want to assign this role to these users?"
msgstr "Sigur doriți să atribuiți acest rol acestor utilizatori?"

msgid "You don't have permission to edit this user"
msgid_plural "You don't have permission to edit these users"
msgstr[0] "Nu aveți permisiunea de a edita acest utilizator"
