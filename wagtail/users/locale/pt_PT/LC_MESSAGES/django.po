msgstr "Conta"

msgid "Roles"
msgstr "Funções"

msgid "Users"
msgstr "Utilizadores"

msgid "Add a user"
msgstr "Adicionar um utilizador"

msgid "Select all users in listing"
msgstr "Selecionar todos os utilizadores da listagem"

msgid "Username"
msgstr "Utilizador"

msgid "Level"
msgstr "Nível"

msgid "Status"
msgstr "Estado"

msgid "Last Login"
msgstr "Último início de sessão"

msgid "Admin"
msgstr "Administração"

msgid "Active"
msgstr "Ativo"

msgid "Inactive"
msgstr "Inativo"

#, python-format
msgid "%(time_period)s ago"
msgstr "%(time_period)s atrás"

#, python-format
msgid "Sorry, no users match \"<em>%(query_string)s</em>\""
msgstr ""
"Desculpe, nenhum utilizador corresponde a \"<em>%(query_string)s</em>\""

#, python-format
msgid ""
"The %(group_name)s group has no users configured. Why not <a href="
"\"%(wagtailusers_add_url)s\">add some</a>?"
msgstr ""
"O grupo %(group_name)s ainda não tem utilizadores configurados. Que tal <a "
"href=\"%(wagtailusers_add_url)s\">adicionar alguns</a>?"

#, python-format
msgid ""
"There are no users configured. Why not <a href=\"%(wagtailusers_add_url)s"
"\">add some</a>?"
msgstr ""
"Ainda não existem utilizadores configurados. Que tal <a href="
"\"%(wagtailusers_add_url)s\">adicionar alguns</a>?"

msgid "Assign role to selected users"
msgstr "Atribuir papel aos utilizadores selecionados"

#, python-format
msgid "%(num_parent_objects)d user has been assigned as %(role)s"
msgid_plural "%(num_parent_objects)d users have been assigned as %(role)s"
msgstr[0] "%(num_parent_objects)d utilizador foi adicionado a %(role)s"
msgstr[1] "%(num_parent_objects)d utilizadores foram adicionados a %(role)s"

msgid "Delete selected users"
msgstr "Apagar utilizadores selecionados"

#, python-format
msgid "%(num_parent_objects)d user has been deleted"
msgid_plural "%(num_parent_objects)d users have been deleted"
msgstr[0] "%(num_parent_objects)d utilizador foi apagado"
msgstr[1] "%(num_parent_objects)d utilizadores foram apagados"

msgid "Change the active state for selected users"
msgstr "Alterar o estado ativo dos utilizadores selecionados"

#, python-format
msgid "%(num_parent_objects)d user has been marked as active"
msgid_plural "%(num_parent_objects)d users have been marked as active"
msgstr[0] "%(num_parent_objects)d utilizador foi marcado como ativo"
msgstr[1] "%(num_parent_objects)d utilizadores foram marcados como ativos"

#, python-format
msgid "%(num_parent_objects)d user has been marked as inactive"
msgid_plural "%(num_parent_objects)d users have been marked as inactive"
msgstr[0] "%(num_parent_objects)d utilizador foi marcado como inativo"
msgstr[1] "%(num_parent_objects)d utilizadores foram marcados como inativos"

