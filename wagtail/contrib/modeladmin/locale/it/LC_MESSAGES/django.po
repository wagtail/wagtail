msgid "Wagtail ModelAdmin"
msgstr "Amministrazione Modelli Wagtail"

msgid "Parent page"
msgstr "Pagina padre"

#, python-format
msgid "Add %s"
msgstr "Aggiungi %s"

#, python-format
msgid "Add a new %s"
msgstr "Aggiungi un nuovo %s"

msgid "Inspect"
msgstr "Controlla"

#, python-format
msgid "Inspect this %s"
msgstr "Controlla questo %s"

msgid "Edit"
msgstr "Modifica"

#, python-format
msgid "Edit this %s"
msgstr "Modifica questo %s"

msgid "Delete"
msgstr "Elimina"

#, python-format
