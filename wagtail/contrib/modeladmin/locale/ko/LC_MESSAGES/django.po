msgid "Wagtail ModelAdmin"
msgstr "웨그테일 모델 관리자"

msgid "Parent page"
msgstr "상위 페이지"

#, python-format
msgid "Add %(object)s"
msgstr "%(object)s 추가"

#, python-format
msgid "Add a new %(object)s"
msgstr "새로운 %(object)s 추가하기"

msgid "Inspect"
msgstr "검사"

#, python-format
msgid "Inspect this %(object)s"
msgstr "이 %(object)s 검사하기"

msgid "Edit"
msgstr "수정"

#, python-format
msgid "Edit this %(object)s"
msgstr "이 %(object)s 수정하기"

msgid "Delete"
msgstr "삭제"

#, python-format
msgid "Delete this %(object)s"
msgstr "이 %(object)s 삭제하기"

msgid "Unpublish"
msgstr "퍼블리싱 해제"

#, python-format
msgid "Unpublish this %(object)s"
msgstr "이 %(object)s의 퍼블리싱 해제"

