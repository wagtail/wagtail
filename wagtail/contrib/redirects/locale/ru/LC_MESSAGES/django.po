msgid "Wagtail redirects"
msgstr "Перенаправления Wagtail"

msgid "Type"
msgstr "Тип"

msgid "Permanent"
msgstr "Постоянный"

msgid "Temporary"
msgstr "Временный"

msgid "All"
msgstr "Все"

