msgid "Add user"
msgstr "Προσθήκη χρήστη"

msgid "Account"
msgstr "Λογαριασμός"

msgid "Roles"
msgstr "Ρόλοι"

msgid "Users"
msgstr "Χρήστες"

msgid "Add a user"
msgstr "Προσθήκη χρήστη"

msgid "Username"
msgstr "Όνομα χρήστη"

msgid "Level"
msgstr "Επίπεδο"

msgid "Status"
msgstr "Κατάσταση"

msgid "Admin"
msgstr "Διαχειριστής"

msgid "Active"
msgstr "Ενεργός"

msgid "Inactive"
msgstr "Μη ενεργός"

