msgid "URL"
msgstr "Διεύθυνση"

msgid "Please enter a valid URL"
msgstr "Παρακαλώ εισάγετε μια διεύθυνση"

msgid "embed"
msgstr "ενσωμάτωση"

msgid "embeds"
msgstr "ενσωματώσεις"

