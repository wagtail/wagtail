msgid "No %(name)s have been created yet."
msgstr "Er zijn nog geen %(name)s aangemaakt."

#, python-format
msgid "Why not <a href=\"%(url)s\">add one</a>?"
msgstr "<a href=\"%(url)s\">Voeg er nu eentje toe!</a>"

msgid "Pagination"
msgstr "Paginering"

#, python-format
msgid "Page %(current_page)s of %(num_pages)s."
msgstr "Pagina %(current_page)s van %(num_pages)s."

msgid "Previous"
msgstr "Vorige"

msgid "Next"
msgstr "Volgende"

#, python-format
msgid "%(model_name)s '%(object)s' created."
msgstr "%(model_name)s '%(object)s' aangemaakt."

#, python-format
msgid "The %(object)s could not be created due to errors."
msgstr "De %(object)s kon niet worden aangemaakt door fouten."

msgid "New"
msgstr "Nieuw"

